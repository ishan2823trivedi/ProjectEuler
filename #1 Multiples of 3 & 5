#include <map>
#include <set>
#include <list>
#include <cmath>
#include <ctime>
#include <deque>
#include <queue>
#include <stack>
#include <string>
#include <bitset>
#include <cstdio>
#include <limits>
#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
#include <unordered_map>

using namespace std;


int main(){
    
    long int t;
    cin >> t;
    long int n;
    for(long int i=0;i<t;i++){
        cin>>n;
        long int sum = 0;
            long int q = (n-1)/3;
            long int p = (n-1)/5;
            long int r = (n-1)/15;
            sum = sum + ((3*q*(q+1))/2) + ((5*p*(p+1))/2) - ((15*r*(r+1))/2) ;
            

        
    cout<<sum<<"\n";

    }
    
    
        
    
    return 0;
}

